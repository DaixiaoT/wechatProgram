<!--pages/contact/contact.wxml-->
<view class="shop-item">
  <view class="thumb">
    <image src="{{me.image}}" mode=""/>
  </view>
  <view class="info">
    <text class="shop-title">{{me.name}}</text>
    <text>电  话：{{me.phone}}</text>
    <text>签  名：{{me.gold}}</text>
  </view>
  
</view>
<view class="shop-item">
  <wemark md="{{me.content}}" highlight="true" type="wemark"></wemark>
  
</view>

